                            }
                        }]
                    },{
                        code: 'div-gpt-ad-1488495260324-10',
                        sizes: [[160, 600]],
                        bids: [
                        {
                            "bidder": "openx",
                            "params": {
                                "unit": "539181152",
                                "delDomain": "coinmarketcap-d.openx.net" // Desktop - 160x600 Detail page
                            }
                        },
                        {
                            "bidder": "appnexus",
                            "params": {
                                "placementId": "11835085" // [D] 160x600 Detail: 11835085
                            }
                        },        
                        {
                            "bidder": "indexExchange",
                            "params": {
                                "id": "5",
                                "siteID": "219068" // coinmarketcap.com_Desktop_160x600_Side Detail
                            }
                        },        
