                            "bidder": "sovrn",
                            "params": {
                                "tagid": "496633" // CoinMarketCap_AiH_160x600_Side_DETAILPAGE_Desktop
                            }
                        }]
                    },{
                        code: 'div-gpt-ad-1488495260324-8',
                        sizes: [[728, 90]],
                        bids: [
                        {
                            "bidder": "openx",
                            "params": {
                                "unit": "539181150",
                                "delDomain": "coinmarketcap-d.openx.net" // Desktop - 728x90 BTF Detail page
                            }
                        },
                        {
                            "bidder": "appnexus",
                            "params": {
                                "placementId": "11835086" // [D] 728x90 Detail BTF: 11835086
                            }
                        },        
                        {
                            "bidder": "indexExchange",
                            "params": {
                                "id": "6",
                                "siteID": "219069" // coinmarketcap.com_Desktop_728x90_BTF Detail
                            }
                        },        
                        {
                            "bidder": "sovrn",
                            "params": {
                                "tagid": "496631" // CoinMarketCap_AiH_728x90_BTF_DETAILPAGE_Desktop
                            }
                        }]
                    }
                
            
            ];

            var apstagSlots = [
            
                
                    {
                        slotID: 'div-gpt-ad-1488495260324-9',
                        slotName: '/48901027/non_landing_leaderboard_top',
                        sizes: [[728, 90]]
                    },
                    {
                        slotID: 'div-gpt-ad-1488495260324-10',
                        slotName: '/48901027/non_landing_side',
                        sizes: [[160, 600]]
                    },
                    {
                        slotID: 'div-gpt-ad-1488495260324-8',
                        slotName: '/48901027/non_landing_leaderboard_bottom',
                        sizes: [[728, 90]]
                    }
                
            
            ];

            var bidders = ['a9', 'prebid'];

            var requestManager = {
                adserverRequestSent: false,
            };
            
                bidders.forEach(function(bidder) {
                requestManager[bidder] = false;
            })

            function allBiddersBack() {
                var allBiddersBack = bidders
                .map(function(bidder) {
                    return requestManager[bidder];
                })
                .filter(Boolean)
                .length === bidders.length;
                return allBiddersBack;
            }
            
            function headerBidderBack(bidder) {
                if (requestManager.adserverRequestSent === true) {
                    return;
                }
                if (bidder === 'a9') {
                    requestManager.a9 = true;
                } else if (bidder === 'prebid') {
                    requestManager.prebid = true;
                }
                if (allBiddersBack()) {
                    sendAdserverRequest();
                }
            }

            function sendAdserverRequest() {
                if (requestManager.adserverRequestSent === true) {
                    return;
                }

                requestManager.adserverRequestSent = true;
                pbjs.adserverRequestSent = true;
                requestManager.sendAdserverRequest = true;
                googletag.cmd.push(function() {
                    apstag.setDisplayBids();
                    if (pbjs.version) {
                        pbjs.setTargetingForGPTAsync();
                    }
                    googletag.pubads().refresh();
                });
            }

            function requestBids(apstagSlots, adUnits, bidTimeout) {
                apstag.fetchBids({
                    slots: apstagSlots,
                    timeout: bidTimeout
                    }, function(bids) {
                        headerBidderBack('a9');
                    });

                // request bids from prebid
                pbjs.que.push(function() {
                    pbjs.setPriceGranularity("high");
                    pbjs.addAdUnits(adUnits);
                    pbjs.enableSendAllBids();
                    pbjs.bidderSettings = {
                      appnexus: {
                        bidCpmAdjustment : function(bidCpm){
                          return bidCpm * 0.66;
                        }
                      },
                      openx: {
                        bidCpmAdjustment : function(bidCpm){
                          return bidCpm * 0.80;
                        }
                      }                      
                    };

                    pbjs.requestBids({
                        bidsBackHandler: function (bids) {
                            headerBidderBack('prebid');
                        }
                    });
                });
            }

            requestBids(apstagSlots, adUnits, bidTimeout)
            window.setTimeout(function() {
                sendAdserverRequest();
            }, bidTimeout);
        };

        fetchHeaderBids();

        </script>
