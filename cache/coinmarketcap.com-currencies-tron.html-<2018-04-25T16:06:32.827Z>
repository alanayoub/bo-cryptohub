                            }
                        },        
                        {
                            "bidder": "sovrn",
                            "params": {
                                "tagid": "496631" // CoinMarketCap_AiH_728x90_BTF_DETAILPAGE_Desktop
                            }
                        }]
                    }
                
            
            ];

            var apstagSlots = [
            
                
                    {
                        slotID: 'div-gpt-ad-1488495260324-9',
                        slotName: '/48901027/non_landing_leaderboard_top',
                        sizes: [[728, 90]]
                    },
                    {
                        slotID: 'div-gpt-ad-1488495260324-10',
                        slotName: '/48901027/non_landing_side',
                        sizes: [[160, 600]]
                    },
