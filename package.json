{
  "name": "cryptohub",
  "author": "Alan Ayoub",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build:dev": "npx webpack --config webpack.dev.js",
    "build:prod": "npx webpack --config webpack.prod.js",
    "build:link": "npm link ../bo-datatable && npm link ../bo-utils",
    "start:dev": "npx babel-node --inspect --use-strict ./src/index.js --logger=true",
    "start:prod": "node --use-strict ./dist/index.js",
    "start:prod-local": "node --inspect --use-strict ./dist/index.js --local=true",
    "start:dev-all": "npm run build:link && npm run build:dev && npm run start:dev",
    "start:prod-all": "npm run build:link && npm run build:prod && npm run start:prod",
    "start:prod-local-all": "npm run build:link && npm run build:prod && npm run start:prod-local"
  },
  "keywords": [
    "Binary Overdose"
  ],
  "dependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.3",
    "@babel/plugin-transform-modules-umd": "^7.2.0",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@octokit/rest": "^15.2.6",
    "ag-grid": "^18.1.2",
    "argv": "0.0.2",
    "await-to-js": "^2.0.1",
    "babel-loader": "^8.0.5",
    "cheerio": "^1.0.0-rc.3",
    "clean-webpack-plugin": "^2.0.1",
    "cookie-parser": "^1.4.4",
    "copy-webpack-plugin": "^5.0.2",
    "es6-promisify": "^6.0.0",
    "eslint": "^5.16.0",
    "express": "^4.16.3",
    "flat": "^4.1.0",
    "fs-extra": "^6.0.0",
    "globby": "^8.0.1",
    "javascript-time-ago": "^1.0.31",
    "mongoose": "^5.4.22",
    "pug": "^2.0.3",
    "pug-cli": "^1.0.0-alpha6",
    "redis": "^2.8.0",
    "replace": "^1.1.0",
    "request": "^2.83.0",
    "request-promise": "^4.2.4",
    "single-line-log": "^1.1.2",
    "webpack-merge": "^4.2.1",
    "winston": "^3.0.0-rc5"
  }
}
