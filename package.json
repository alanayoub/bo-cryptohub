  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "build:client-dev": "npx webpack --config webpack.client.dev.js",
    "build:client-prod": "npx webpack --config webpack.client.prod.js",
    "build:server-dev": "npx webpack --config webpack.server.dev.js",
    "build:server-prod": "npx webpack --config webpack.server.prod.js",
    "build:dev": "npm run build:client-dev && npm run build:server-dev",
    "build:prod": "npm run build:client-prod && npm run build:server-prod",
    "build:link": "npm link ../bo-datatable && npm link ../bo-utils",
    "start:dev": "npx babel-node --inspect --use-strict ./src/index.js --local=true --logger=true",
    "start:prod": "node --use-strict ./dist/index.js",
    "start:prod-local": "node --inspect --use-strict ./dist/index.js --local=true --logger=true",
    "start:dev-all": "npm run build:link && npm run build:dev && npm run start:dev",
    "start:prod-all": "npm run build:link && npm run build:prod && npm run start:prod",
    "start:prod-local-all": "npm run build:link && npm run build:prod && npm run start:prod-local"
  },
  "keywords": [
    "Binary Overdose"
  ],
